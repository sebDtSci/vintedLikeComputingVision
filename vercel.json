{
  "version": 2,
  "builds": [{
    "src": "app.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "50mb" }
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "app.py"
  }],
  "ignore": [
    "model/rnn/",
    "model/modelReco.py",
    "requirementsMac.txt",
    "requirementsPc.txt"
  ]
}

